---
import ButtonNameFloating from "./ButtonNameFloating.astro";
const { skills } = Astro.props;
---

<div class="flex flex-row gap-2">
  {
    skills.map((item) => (
      <ButtonNameFloating message={item.skill.toUpperCase()} className={true}>
        <img class='w-7 h-7' src={item.url} alt={item.skill} />
      </ButtonNameFloating>
    ))
  }
</div>
